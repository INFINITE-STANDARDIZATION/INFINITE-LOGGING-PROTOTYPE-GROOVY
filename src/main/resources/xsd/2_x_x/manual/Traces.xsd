<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified"
           targetNamespace="https://i-t.io/logging/groovy/2_x_x/Main"
           xmlns="https://i-t.io/logging/groovy/2_x_x/Main"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="../Event.xsd"/>
    <xs:include schemaLocation="../Trace.xsd"/>
    <xs:complexType name="Traces">
        <xs:complexContent>
            <xs:extension base="Event">
                <xs:sequence>
                    <xs:element name="trace" type="Trace" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>