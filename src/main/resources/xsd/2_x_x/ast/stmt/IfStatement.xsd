<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified"
           targetNamespace="https://i-t.io/logging/groovy/2_x_x/Main"
           xmlns="https://i-t.io/logging/groovy/2_x_x/Main"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="Statement.xsd"/>
    <xs:include schemaLocation="../expr/BooleanExpression.xsd"/>
    <xs:complexType name="IfStatement">
        <xs:complexContent>
            <xs:extension base="Statement">
                <xs:sequence>
                    <xs:element minOccurs="0" name="variable" type="BooleanExpression"/>
                    <xs:element minOccurs="0" name="ifBlock" type="Statement"/>
                    <xs:element minOccurs="0" name="elseBlock" type="Statement"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>