<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified"
           targetNamespace="https://i-t.io/logging/groovy/2_x_x/Main"
           xmlns="https://i-t.io/logging/groovy/2_x_x/Main"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="ExecutionWithCode.xsd"/>
    <xs:include schemaLocation="../Trace.xsd"/>
    <xs:complexType name="ExpressionEvaluation">
        <xs:complexContent>
            <xs:extension base="ExecutionWithCode">
                <xs:sequence>
                    <xs:element type="Trace" name="expressionResult" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="expressionTypeClassName" type="xs:string" use="required"/>
                <xs:attribute name="expressionName" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>